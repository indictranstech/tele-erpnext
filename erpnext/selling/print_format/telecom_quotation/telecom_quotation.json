 "html": "<html>\n  <body>\n      <div class=\"row\">\n          <div class=\"col-xs-12\">\n              <div class=\"row\">\n                  <div class=\"col-xs-8\">\n                      <!-- Logo and address -->\n                      <table width=100% style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n                          <tr>\n                              <td width=\"60%\"><img src=\"files/logo.png\" height=\"10%\"></td>\n                              <td width=\"40%\"><h5 style=\"margin:0;cellpadding:0\"><b>Telecom Advocates, Inc</b></h5>\n                                  <br>2625 Cumberland Parkway,\n                                  <br>Suite 270,\n                                  <br>Atlanta, GA 30339\n                                  <br>Tel: (404) 998-5950\n                                  <br>Fax: (404) 998-5951\n                                  <br>Email: sales@telecom-advocates.com\n                              </td>\n                          </tr>\n                      </table>\n                  </div>\n                  <div class=\"col-xs-4\">\n                      <!-- Quotation Details  -->\n                      <table width=100% class=table-bordered style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n                          <th colspan=\"2\">Quote</th>\n                          <tr >\n                             <td width=40%><b>Date:</b></td>\n                             <td width=60% align=\"left\">{{ doc.get_formatted(\"transaction_date\") or \"\" }}</td>\n                          </tr>\n                          <tr>\n                             <td width=40% ><b>Quote#</b></td>\n                             <td width=60% align=\"left\">{{ doc.name or \"\" }}</td>\n                          </tr>\n                      </table>\n                  </div>\n              </div>\n          </div>\n      </div>\n      <br>\n      <div class=\"row\">\n          <div class=\"col-xs-6\">\n              <!-- Bill To details -->\n              <table width=100% class=table-bordered style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n                  <th><b>Name / Address</b></th>\n                  <tr>\n                      <td>\n                          {{ doc.customer_name or \"\" }}\n                      <br>{{ doc.address_display or \"\" }}\n                      </td>\n                  </tr>\n              </table>\n          </div>\n          <div class=\"col-xs-6\">\n              <!-- Ship To Details  -->\n              <table width=100% class=table-bordered style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n                  <th><b>Ship To</b></th>\n                  <tr>\n                      <td>\n                          {{ doc.contact_display or \"\" }}\n                      <br>{{ doc.shipping_address or \"\" }}\n                      </td>\n                  </tr>\n              </table>\n          </div>\n      </div>\n      <br>\n      <div class=\"row\">\n          <div class=\"col-xs-6\">\n          </div>\n          <div class=\"col-xs-6\">\n              <!-- Terms Table  -->\n              <table width=100% class=table-bordered style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n                  <th>Term</th>\n                  <th>Rep</th>\n                  <th>Project</th>\n                  <tr>\n                      <td>{{ doc.tc_name or \"\" }}</td>\n                      <td>{{ doc.employee or \"\"}} - {{ doc.employee_name}}</td>\n                      <td></td>\n                  </tr>\n              </table>\n          </div>\n      </div>\n      <br>\n      <div class=\"row\">\n          <div class=\"col-xs-12\">\n              <!-- Item Table -->\n              <table width=100% class=table-bordered style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n                  <th>Item</th>\n                  <th>Rate</th>\n                  <th>Description</th>\n                  <th>Qty</th>\n                  <th>Total</th>\n                  {%- for row in doc.items -%}\n                  <tr>\n                      <td>{{ row.item_name or \"\" }}</td>\n                      <td>{{ row.rate or \"\" }}</td>\n                      <td>{{ row.description or \"\" }}</td>\n                      <td>{{ row.qty or \"\" }}</td>\n                      <td>{{ row.amount or \"\" }}</td>\n                  </tr>\n                  {%- endfor -%}\n                  <tr>\n                      <td colspan=\"3\"></td>\n                      <td colspan=\"2\" align=\"center\">\n                          <b>Total :</b> {{ frappe.db.get_value(\"Currency\", doc.currency, \"symbol\") or \"\" }} {{doc.rounded_total or \"\"}}\n                      </td>\n                  </tr>\n              </table>\n          </div>\n      </div>\n  </body>\n  <footer align=\"center\">\n      <p>www.telecom-advocates.com</p>\n  </footer>\n</html>", 
