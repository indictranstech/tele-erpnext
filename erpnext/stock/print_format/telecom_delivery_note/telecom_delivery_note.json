 "html": "{{ letter_head }}\n\n{%- if trigger_print -%}\n<script>\nwindow.print();\n\n// close the window after print\n// NOTE: doesn't close if print is cancelled in Chrome\nwindow.onfocus = function() {\nwindow.close();\n}\n</script>\n{%- endif -%}\n\n<div class=\"row\">\n    <div class=\"col-xs-6\"></div>\n    <div class=\"col-xs-6\" align=\"right\">\n        <div class=\"row\">\n            <div class=\"col-xs-12\">\n                <h4 style=\"margin:0;cellpadding:0\" align=\"right\"><b>PACKING SLIP</b></h4>\n            </div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-xs-12\">\n<table width=60% class=table-bordered style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n              <th>Date</th>\n              <th>Note#</th>\n              <tr>\n                  <td>{{ doc.get_formatted(\"posting_date\",doc) or \"\" }}</td>\n                  <td>{{ doc.name or \"\" }} </td>\n              </tr>\n           </table>\n</div>\n        </div>\n        </div>\n    <div class=\"col-xs-4\"></div>\n</div>\n<hr>\n<div class=\"row\">\n  <div class=\"col-xs-8\"></div>\n  <!-- <div class=\"col-xs-8\">\n      <table width=100% class=table-bordered style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n          <th>PO No.</th>\n          <th>Ship</th>\n          <th>Via</th>\n          <th>FOB</th>\n          <tr>\n              <td>{{ doc.po_no or \"\" }}</td>\n              <td>{{ doc.get_formatted(\"posting_date\") or \"\" }}</td>\n              <td>{{ doc.via or \"\" }}</td>\n              <td>{{ doc.fob or \"\" }}</td>\n          </tr>\n      </table>\n  </div> -->\n  <div class=\"col-xs-4\">\n      <!-- Ship To Details  -->\n      <table width=100%  class=table-bordered style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n          <th><b>Ship To</b></th>\n          <tr>\n              <td style=\"height: 130px;display: block;\">\n                  {{ doc.contact_display or \"\" }}\n              <br>{{ doc.shipping_address or \"\" }}\n            </td>\n          </tr>\n      </table>\n  </div>\n</div>\n<br>\n<div class=\"row\">\n<div class=\"col-xs-12\">\n    <table width=100% class=table-bordered style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n      <th>PO No.</th>\n      <th>Ship</th>\n      <th>Via</th>\n      <th>FOB</th>\n      <tr>\n          <td>{{ doc.po_no or \"\" }}</td>\n          <td>{{ doc.get_formatted(\"posting_date\") or \"\" }}</td>\n          <td>{{ doc.via or \"\" }}</td>\n          <td>{{ doc.fob or \"\" }}</td>\n      </tr>\n    </table>\n</div>\n</div>\n<div class=\"row\">\n  <div class=\"col-xs-12\">\n      <!-- Item Table -->\n      <table width=100% class=table-bordered style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n          <th>Quantity</th>\n          <th>Item Code</th>\n          <th>Description</th>\n          {%- for row in doc.items -%}\n          <tr>\n              <td align=\"right\">{{ row.qty or \"\" }}</td>\n              <td>{{ row.item_code or \"\" }}</td>\n              <td>{{ row.description or \"\" }}</td>\n          </tr>\n          {%- endfor -%}\n          <tr rowspan=\"2\">\n              <td colspan=\"3\" height=\"35px\"></td>\n          </tr>\n      </table>\n  </div>\n</div>\n<br>\n<footer align=\"center\">\n  <p>www.telecom-advocates.com</p>\n</footer>", 
